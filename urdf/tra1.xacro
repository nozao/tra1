<?xml version="1.0"?>
<!-- Revolute-Revolute Manipulator -->
<robot name="rrbot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Constants for robot dimensions -->
  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="RIGHT_ANGLE" value="1.570796327"/>

  <!-- Constants for TRA1 -->
  <xacro:property name="FL_J1X" value="0"/>
  <xacro:property name="J1_J2X" value="0"/>
  <xacro:property name="J2_J3X" value="0"/>
  <xacro:property name="J3_J4X" value="0"/>
  <xacro:property name="J4_J5X" value="0"/>
  <xacro:property name="J5_J6X" value="-0.003"/>

  <xacro:property name="FL_J1Y" value="0"/>
  <xacro:property name="J1_J2Y" value="0.05"/>
  <xacro:property name="J2_J3Y" value="0"/>
  <xacro:property name="J3_J4Y" value="0"/>
  <xacro:property name="J4_J5Y" value="0"/>
  <xacro:property name="J5_J6Y" value="0"/>

  <xacro:property name="FL_J1Z" value="0.187"/>
  <xacro:property name="J1_J2Z" value="0.168"/>
  <xacro:property name="J2_J3Z" value="0.440"/>
  <xacro:property name="J3_J4Z" value="0.2315"/>
  <xacro:property name="J4_J5Z" value="0.1435"/>
  <xacro:property name="J5_J6Z" value="0.1825"/>


  <!-- Import Rviz colors -->
  <xacro:include filename="$(find techno21_robot)/urdf/materials.xacro" />

  <!-- Used for fixing robot to Gazebo 'base_link' -->
  <link name="world"/>

  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="link11"/>
  </joint>

<!-- ==================================================================link1 -->
  <link name="link11">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 0"/>
        <geometry>
            <mesh filename="package://techno21_robot/meshes/link1.STL" scale="1 1 1"/>
        </geometry>
        <material name="grey"/>
    </visual>
  </link>

<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++joint1 -->
  <joint name="joint1" type="continuous">
    <parent link="link11"/>
    <child link="link2"/>
    <origin xyz="${FL_J1X} ${FL_J1Y} ${FL_J1Z}" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.7"/>
  </joint>

<!-- ==================================================================link2 -->
  <link name="link2">
    <visual>
      <origin xyz="${-FL_J1X} ${-FL_J1Y} ${-FL_J1Z}" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://techno21_robot/meshes/link2.STL" scale="1 1 1"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++joint2 -->
  <joint name="joint2" type="continuous">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="${J1_J2X} ${J1_J2Y} ${J1_J2Z}" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <dynamics damping="0.7"/>
  </joint>

<!-- ==================================================================link3 -->
    <link name="link3">
        <visual>
          <origin xyz="${-(FL_J1X+J1_J2X)} ${-(FL_J1Y+J1_J2Y)} ${-(FL_J1Z+J1_J2Z)}" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://techno21_robot/meshes/link3.STL" scale="1 1 1"/>
          </geometry>
          <material name="grey"/>
        </visual>
    </link>

<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++joint3 -->
    <joint name="joint3" type="continuous">
      <parent link="link3"/>
      <child link="link4"/>
      <origin xyz="${J2_J3X} ${J2_J3Y} ${J2_J3Z}" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <dynamics damping="0.7"/>
    </joint>

<!-- ==================================================================link4 -->
    <link name="link4">
        <visual>
          <origin xyz="${-(FL_J1X+J1_J2X+J2_J3X)} ${-(FL_J1Y+J1_J2Y+J2_J3Y)} ${-(FL_J1Z+J1_J2Z+J2_J3Z)}" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://techno21_robot/meshes/link4.STL" scale="1 1 1"/>
          </geometry>
          <material name="grey"/>
        </visual>
    </link>

<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++joint4 -->
    <joint name="joint4" type="continuous">
      <parent link="link4"/>
      <child link="link5"/>
      <origin xyz="${J3_J4X} ${J3_J4Y} ${J3_J4Z}" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <dynamics damping="0.7"/>
    </joint>

<!-- ==================================================================link5 -->
    <link name="link5">
        <visual>
          <origin xyz="${-(FL_J1X+J1_J2X+J2_J3X+J3_J4X)} ${-(FL_J1Y+J1_J2Y+J2_J3Y+J3_J4Y)} ${-(FL_J1Z+J1_J2Z+J2_J3Z+J3_J4Z)}" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://techno21_robot/meshes/link5.STL" scale="1 1 1"/>
          </geometry>
          <material name="black"/>
        </visual>
    </link>
<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++joint5 -->
    <joint name="joint5" type="continuous">
      <parent link="link5"/>
      <child link="link6"/>
      <origin xyz="${J4_J5X} ${J4_J5Y} ${J4_J5Z}" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <dynamics damping="0.7"/>
    </joint>

<!-- ==================================================================link6 -->
    <link name="link6">
        <visual>
          <origin xyz="${-(FL_J1X+J1_J2X+J2_J3X+J3_J4X+J4_J5X)} ${-(FL_J1Y+J1_J2Y+J2_J3Y+J3_J4Y+J4_J5Y)} ${-(FL_J1Z+J1_J2Z+J2_J3Z+J3_J4Z+J4_J5Z)}" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://techno21_robot/meshes/link6.STL" scale="1 1 1"/>
          </geometry>
          <material name="grey"/>
        </visual>
    </link>

<!-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++joint6 -->
    <joint name="joint6" type="continuous">
      <parent link="link6"/>
      <child link="link7"/>
      <origin xyz="${J5_J6X} ${J5_J6Y} ${J5_J6Z}" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <dynamics damping="0.7"/>
    </joint>

<!-- ==================================================================link7 -->
    <link name="link7">
        <visual>
          <origin xyz="${-(FL_J1X+J1_J2X+J2_J3X+J3_J4X+J4_J5X+J5_J6X)} ${-(FL_J1Y+J1_J2Y+J2_J3Y+J3_J4Y+J4_J5Y+J5_J6Y)} ${-(FL_J1Z+J1_J2Z+J2_J3Z+J3_J4Z+J4_J5Z+J5_J6Z)}" rpy="0 0 0"/>
          <geometry>
            <mesh filename="package://techno21_robot/meshes/link7.STL" scale="1 1 1"/>
          </geometry>
          <material name="black"/>
        </visual>
    </link>

</robot>
